{
    "git": {
        "commitMessage": "chore(release): release v${version} :tada:",
        "requireBranch": "main"
    },
    "github": {
        "release": true
    },
    "npm": {
        "publish": false,
        "autoGenerate": true,
        "web": true
    },
    "plugins": {
        "@release-it/conventional-changelog": {
            "preset": "angular",
            "infile": "CHANGELOG.md"
        }
    }
}