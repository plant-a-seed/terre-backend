{
    "compilerOptions": {
        "strict": true,
        "target": "ES2015",
        "module": "Node16",
        "moduleResolution": "node16",
        "lib": [
            "ES2021"
        ],
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "sourceMap": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./*"
            ],
            "@bot/*": [
                "./bot/*"
            ],
            "@options": [
                "./utils/options.js"
            ],
            "@types": [
                "./utils/types.js"
            ]
        },
        "outDir": "../dist"
    }
}